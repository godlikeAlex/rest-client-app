{
  "name": "rest-react-router",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build && node netlify/prepare.js",
    "dev": "cross-env NODE_ENV=development node ./dev-server.js",
    "start": "netlify serve",
    "typecheck": "react-router typegen && tsc",
    "format:fix": "prettier . -w -l -u",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@netlify/functions": "^3.1.10",
    "@react-router/node": "^7.7.1",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@react-router/dev": "^7.7.1",
    "@remix-run/node-fetch-server": "0.8.0",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/express": "^5.0.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitest/eslint-plugin": "^1.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.6.8",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "netlify-cli": "^1.2.3",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.42.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
